{
    "baseBranches": [
        "main"
    ],
    "configWarningReuseIssue": false,
    "dependencyDashboard": true,
    "dependencyDashboardTitle": "Renovate: Upgrade Gitlab Dependencies",
    "draftPR": true,
    "branchTopic": "ironbank",
    "enabledManagers": [
        "helm-values",
        "custom.regex"
    ],
    "ignorePaths": [
        "chart/charts/**",
        "chart/examples/**",
        "chart/scripts/**"
    ],
    "labels": [
        "gitlab",
        "renovate",
        "priority::2",
        "kind::maintenance"
    ],
    "packageRules": [
        {
            "matchManagers": ["custom.regex"],
            "groupName": "Ironbank"
        },
        {
            "matchDatasources": ["docker"],
            "groupName": "Ironbank"
        },
        {
            "matchDatasources": [
                "docker"
            ],
            "matchPackageNames": [
                "registry1.dso.mil/ironbank/bitnami/postgres"
            ],
            "allowedVersions": "<=17"
        },
        {
            "matchPackageNames": [
                "registry1.dso.mil/ironbank/opensource/minio/minio"
            ],
            "versioning": "regex:(RELEASE\\.(?<major>\\d{4})-(?<minor>\\d{2})-(?<patch>\\d{2})T(?<build>\\d{2}-\\d{2}-\\d{2})Z)",
            "allowedVersions": "RELEASE.2024-06-04T19-20-08Z"
        },
        {
            "matchPackageNames": [
                "registry1.dso.mil/ironbank/opensource/minio/mc"
            ],
            "versioning": "regex:(RELEASE\\.(?<major>\\d{4})-(?<minor>\\d{2})-(?<patch>\\d{2})T(?<build>\\d{2}-\\d{2}-\\d{2})Z)",
            "allowedVersions": "RELEASE.2024-10-02T08-27-28Z"
        }
    ],
    "customManagers": [
        {
            "customType": "regex",
            "managerFilePatterns": [
                "/^chart/Chart\\.yaml$/"
            ],
            "matchStrings": [
                "appVersion:[^\\S\\r\\n]+(?<currentValue>.+)"
            ],
            "depNameTemplate": "registry1.dso.mil/ironbank/gitlab/gitlab/gitlab-webservice",
            "datasourceTemplate": "docker"
        },
        {
            "customType": "regex",
            "managerFilePatterns": [
                "/^chart/Chart\\.yaml$/"
            ],
            "matchStrings": [
                "image:[^\\S\\r\\n]+(?<depName>.+):(?<currentValue>.+)"
            ],
            "datasourceTemplate": "docker"
        },
        {
            "customType": "regex",
            "managerFilePatterns": [
                "/^chart/values\\.yaml$/"
            ],
            "matchStrings": [
                "repository:\\s*(?<depName>registry1[^\\s]+)[\\s\\S]*?tag:\\s*[\"']?(?<currentValue>[^\\s\"']+)"
            ],
            "datasourceTemplate": "docker"
        },
        {
            "customType": "regex",
            "managerFilePatterns": [
                "/^chart/values\\.yaml$/"
            ],
            "matchStrings": [
                "image:\\s*(?<depName>registry1[^\\s]+)\\s+tag:\\s*[\"']?(?<currentValue>[^\\s\"']+)"
            ],
            "datasourceTemplate": "docker"
        },
        {
            "customType": "regex",
            "managerFilePatterns": [
                "/^chart/values\\.yaml$/"
            ],
            "matchStrings": [
                "registry:\\s*(?<registry>registry1[^\\s]+)[\\s\\S]*?repository:\\s*(?<depName>[^\\s]+)[\\s\\S]*?tag:\\s*[\"']?(?<currentValue>[^\\s\"']+)"
            ],
            "depNameTemplate": "{{{registry}}}/{{{depName}}}",
            "datasourceTemplate": "docker"
        },
        {
            "customType": "regex",
            "managerFilePatterns": [
                "/^tests/images\\.txt$/"
            ],
            "matchStrings": [
                "s*(?<depName>.+):(?<currentValue>.+)"
            ],
            "datasourceTemplate": "docker"
        },
        {
            "customType": "regex",
            "managerFilePatterns": [
                "/^chart/Chart\\.yaml$/"
            ],
            "matchStrings": [
                "- Gitlab:\\s+(?<currentValue>.+)"
            ],
            "registryUrlTemplate": "https://registry1.dso.mil",
            "depNameTemplate": "ironbank/gitlab/gitlab/gitlab-webservice",
            "datasourceTemplate": "docker"
        },
        {
            "customType": "regex",
            "managerFilePatterns": [
                "/^chart/requirements\\.yaml$/"
            ],
            "matchStrings": [
                "- name:[\\s]+gluon[\\s]+version:[\\s]+(?<currentValue>.*)"
            ],
            "registryUrlTemplate": "https://registry1.dso.mil",
            "depNameTemplate": "bigbang/gluon",
            "datasourceTemplate": "docker"
        }
    ],
    "separateMajorMinor": false,
    "separateMultipleMajor": false,
    "postUpdateOptions": [
        "helmUpdateSubChartArchives"
    ]
}
